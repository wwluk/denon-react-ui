{
  "name": "redux-simple-starter",
  "version": "1.0.0",
  "description": "Simple starter package for Redux with React and Babel support",
  "main": "index.js",
  "repository": "git@github.com:StephenGrider/ReduxSimpleStarter.git",
  "scripts": {
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --host 0.0.0.0",
    "build": "run-p build:server build:client",
    "build:client": "cross-env NODE_ENV=production webpack -p",
    "build:server": "cross-env NODE_ENV=production webpack -p --config webpack.server.config.js",
    "test": "mocha --require babel-core/register --require ./test/test_helper.js --require ignore-styles \"test/**/*.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "ignore-styles": "^5.0.1",
    "jquery": "^3.3.1",
    "jsdom": "^8.1.0",
    "json-loader": "^0.5.7",
    "mocha": "^6.0.2",
    "npm-run-all": "^4.1.1",
    "react-addons-test-utils": "^15.2.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "axios": "^0.11.0",
    "babel-preset-stage-1": "^6.1.18",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.7",
    "express": "^4.15.3",
    "http-proxy-middleware": "latest",
    "lodash": "^4.17.11",
    "node-sass": "^4.11.0",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-fontawesome": "^1.5.0",
    "react-redux": "^4.0.0",
    "react-router": "^2.0.1",
    "redux": "^3.0.4",
    "redux-promise-middleware": "^3.0.2",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.13.1",
    "x2js": "^2.0.1"
  }
}
